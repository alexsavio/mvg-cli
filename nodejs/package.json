    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "rimraf": "^2.4.1",
    "shelljs": "^0.5.1",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
